{
  "name": "quizzes",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "env-cmd -f .env/.env.development next dev --turbo",
    "start:dev": "env-cmd -f .env/.env.development next dev",
    "build:dev": "env-cmd -f .env/.env.development next build",
    "start:stag": "env-cmd -f .env/.env.staging next dev",
    "build:stag": "env-cmd -f .env/.env.staging next build",
    "start:main": "env-cmd -f .env/.env.production next start",
    "build:main": "env-cmd -f .env/.env.production next build",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write .",
    "postinstall": "husky",
    "pre-commit": "lint-staged",
    "commitlint": "commitlint --edit"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.19",
    "@hookform/resolvers": "^3.3.4",
    "@mysten/sui.js": "^0.51.2",
    "@mysten/zklogin": "^0.5.2",
    "@polymedia/suits": "^0.0.39",
    "@polymedia/webutils": "^0.0.48",
    "@react-spring/web": "^9.7.3",
    "@reduxjs/toolkit": "^2.2.3",
    "axios": "^1.6.8",
    "crypto-js": "^4.2.0",
    "jwt-decode": "^4.0.0",
    "lodash": "^4.17.21",
    "next": "14.2.2",
    "next-pwa": "^5.6.0",
    "react": "^18",
    "react-dom": "^18",
    "react-flip-toolkit": "^7.1.0",
    "react-hook-form": "^7.51.3",
    "react-loader-spinner": "^6.1.6",
    "react-qr-code": "^2.0.12",
    "react-redux": "^9.1.1",
    "react-toastify": "^10.0.5",
    "sass": "^1.77.1",
    "socket.io-client": "^4.7.5",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@types/crypto-js": "^4.2.2",
    "@types/lodash": "^4.17.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "env-cmd": "^10.1.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.12",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  },
  "engines": {
    "node": "^18.18.0 || >=20.0.0"
  }
}
